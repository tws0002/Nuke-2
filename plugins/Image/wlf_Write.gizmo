            ret = ret or autolabel()
            return ret

        @staticmethod
        def knob_changed():
            if nuke.thisKnob() is nuke.thisNode()['use_custom_frame']:
                nuke.thisNode()['custom_frame'].setEnabled(nuke.thisKnob().value())

        @staticmethod
        def create_output_dir(node):
            _dir = os.path.abspath(
                '{}/../'.format(nuke.filename(node)))
            if not os.path.exists(_dir):
                os.makedirs(_dir)

        @staticmethod
        def is_reading_exr():
            import os
            import sys
            if os.getenv('WLF_IS_SEPARATE_JOBS') or nuke.numvalue('parent.reading_exr'):
                filename = nuke.filename(nuke.thisNode(), nuke.REPLACE).decode('utf-8')
                ret = os.path.exists(filename.encode(sys.getfilesystemencoding()))
                if not ret:
                    nuke.tprint('EXR file not exists, will read input: {}'.format(filename))
            else:
                ret = False
            return ret

